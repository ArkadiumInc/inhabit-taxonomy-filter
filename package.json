{
  "name": "inhabit-taxonomy-filter",
  "version": "1.2.1",
  "description": "taxonomy filter for inhabit",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "prepublish": "npm run test && npm run size",
    "test": "npm run build && mocha ./dist/*.spec.js ./dist/**/*.spec.js",
    "size": "size-limit",
    "test-watch": "concurrently \"tsc --watch\" \"ava dist/**/*.spec.js --watch\""
  },
  "git": "https://github.com/ArkadiumInc/inhabit-taxonomy-filter",
  "author": "wishmasterts",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.42",
    "concurrently": "^3.5.0",
    "cz-conventional-changelog": "^2.0.0",
    "mocha": "^3.5.0",
    "rimraf": "^2.6.1",
    "size-limit": "^0.11.0",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "chai": "^4.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "2 KB"
    }
  ]
}
